<body>
    <div class="main-wrapper set_color" id="main-wrapper">
        <!-- ============================================================== -->
        <!-- Preloader - style you can find in spinners.css -->
        <!-- ============================================================== -->
        <div id="pre-load" hidden>
            <div class="loading">
                <div class="preloader-wrapper big active">
                    <div class="spinner-layer spinner-blue-only">
                        <div class="circle-clipper left">
                            <div class="circle"></div>
                        </div><div class="gap-patch">
                            <div class="circle"></div>
                        </div><div class="circle-clipper right">
                            <div class="circle"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="preloader">
            <div class="loading">
                <div class="preloader-wrapper big active">
                    <div class="spinner-layer spinner-blue-only">
                        <div class="circle-clipper left">
                            <div class="circle"></div>
                        </div><div class="gap-patch">
                            <div class="circle"></div>
                        </div><div class="circle-clipper right">
                            <div class="circle"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <header class="topbar">

            <nav>
                <div class="nav-wrapper">
                    <a href="#!" class="brand-logo" style="background: #ffffff !important;"> <img class="light-logo" src="~/assets/images/Logo.jpg"></a>
                    <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
                    <ul class="right hide-on-med-and-down">
                        <li><a href="/Home/index"  class="nav-link"><i class="material-icons dp48">home</i><span>Home</span></a></li>
                        <li><a href="/giay-hen/nhap-giay-hen/thong-tin-thu-tuc"  class="nav-link"><i class="material-icons dp48">add_circle</i>Nhập giấy hẹn mới</a></li>
                        <li><a href="/giay-hen/tim-giay-hen"  class="nav-link"><i class="material-icons dp48">search</i>Tra cứu giấy hẹn</a></li>
                        <li><a href="/khach-hang/dang-nhap"  class="nav-link"><i class="material-icons dp48">exit_to_app</i>Đăng nhập</a></li>
                    </ul>
                </div>
            </nav>

            <ul class="sidenav" id="mobile-demo">
                <li><a href="/Home/index"><i class="material-icons dp48">home</i>Home</a></li>
                <li><a href="/giay-hen/nhap-giay-hen/thong-tin-thu-tuc"><i class="material-icons dp48">add_circle</i>Nhập giấy hẹn mới</a></li>
                <li><a href="/giay-hen/tim-giay-hen"><i class="material-icons dp48">search</i>Tra cứu giấy hẹn</a></li>
                <li><a href="/khach-hang/dang-nhap"><i class="material-icons dp48">exit_to_app</i>Đăng nhập</a></li>
            </ul>
        </header>

        <div class="page_wrapper content_all">
            @RenderBody()
        </div>
    </div>
    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@

    @*<script src="~/Scripts/Scripts.js"></script>*@



    @*@Scripts.Render("~/Scripts/moment.js")*@
